<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>
<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="vetcare.gui.DonoController" prefHeight="400.0" prefWidth="600.0">
	<GridPane hgap="10">
		<columnConstraints>
			<ColumnConstraints percentWidth="0"/>
			<ColumnConstraints halignment="CENTER" hgrow="ALWAYS"/>
		</columnConstraints>
		<Text      GridPane.rowIndex="0" GridPane.columnIndex="0">Nome</Text>
		<TextField GridPane.rowIndex="0" GridPane.columnIndex="1" fx:id="donoNome"/>

		<Text      GridPane.rowIndex="1" GridPane.columnIndex="0">CPF</Text>
		<TextField GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="donoCPF"/>

		<Text      GridPane.rowIndex="2" GridPane.columnIndex="0">Contato</Text>
		<TextField GridPane.rowIndex="2" GridPane.columnIndex="1" fx:id="donoContato"/>
	</GridPane>
</StackPane>
